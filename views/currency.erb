<h1>Convert <%=  @original_currency %> </h1>

<p>Go <a href="/">back</a>.<p>

<ul>
  <% @get_currency_array.each do |x| %>
    <% if x != @original_currency %>
      <li>
        <a href="<%= @original_currency %>/<%= x %>">Convert 1 <%= @original_currency %> to <%= x %> ...</a>
      </li>
    <% end %>
  <% end %>
</ul>
